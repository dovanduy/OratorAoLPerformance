<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:Profiler.Analyzer.Performance"
	x:Class="Profiler.Analyzer.Performance.Views.ListHeaderItemView" d:DesignHeight="480" Height="20" Width="153.433">
	<UserControl.Resources>
		<!--<local:ListHeaderItemViewModel x:Key="ListHeaderItemViewModelDataSource" />-->
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" Margin="0">
		<Grid.Background>
			<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
				<GradientStop Color="#FFEFEDED"/>
				<GradientStop Color="{DynamicResource {x:Static SystemColors.ScrollBarColorKey}}" Offset="0.507"/>
				<GradientStop Color="White" Offset="1"/>
			</LinearGradientBrush>
		</Grid.Background>
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
			<ColumnDefinition Width="Auto" MinWidth="88.843"/>
		</Grid.ColumnDefinitions>
		<TextBlock Margin="0" TextWrapping="Wrap" Text="Session" Padding="3,0,0,0" FontFamily="Stencil" VerticalAlignment="Center"/>
		<ToggleButton Content="Setting" Grid.Column="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Padding="0" Margin="42,0,0,0"/>
	</Grid>
</UserControl>
